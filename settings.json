{
  "[django-html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    }
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.formatOnType": true
  },
  "[json][jsonc]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "background.enabled": true,
  "background.fullscreen": {
    "images": ["file:///home/luanthnh/Public/Workspaces/vscode/bg.png"], // urls of your images
    "opacity": 0.9, // 0.85 ~ 0.95 recommended
    "size": "cover", // also css, `cover` to self-adaption (recommended)，or `contain`、`200px 200px`
    "position": "center", // alias to `background-position`, default `center`
    "interval": 0 // seconds of interval for carousel, default `0` to disabled.
  },
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.language": "en,vi,lorem",
  "color-highlight.markerType": "underline",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingDelete": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 1,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.addExtraSpaceOnTop": true,
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "Cascadia Code, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontWeight": "500",
  "editor.guides.bracketPairs": "active",
  "editor.hideCursorInOverviewRuler": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.scrollPredominantAxis": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 15,
  "editor.stickyTabStops": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 120,
  "emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "eslint.format.enable": true,
  "eslint.validate": ["javascript"],
  "explorer.confirmDelete": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.views.branches.branches.layout": "list",
  "highlight-matching-tag.styles": {
    "opening": {
      "name": {
        "highlight": "rgba(243, 48, 94, 0.3)"
      }
    }
  },
  "html-css-class-completion.enableEmmetSupport": true,
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "htmltagwrap.tag": "div",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.jsxAttributeCompletionStyle": "auto",
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "material-icon-theme.folders.color": "#7156b4",
  "material-icon-theme.opacity": 1,
  "prettier.printWidth": 120,
  // "prettier.singleAttributePerLine": true,
  "prettier.singleQuote": true,
  "python.formatting.autopep8Args": ["--max-line-length", "120", "--experimental"],
  "python.formatting.provider": "autopep8",
  "python.linting.pylintArgs": ["--max-line-length", "120", "--aggressive"],
  "python.linting.pylintEnabled": true,
  "rest-client.fontFamily": "Cascadia Code, Consolas, 'Courier New', monospace",
  "rest-client.fontSize": 14,
  "rest-client.fontWeight": "700",
  "scm.defaultViewSortKey": "name",
  "search.seedWithNearestWord": true,
